.section.menu  {
    width: 200rpx;
    box-sizing: border-box;
}

.menu .menu-box {
    background-color: #F4F4F4;
}

.menu .menu-box .one-menu {
    font-size: 26rpx;
    padding: 30rpx;
    display: block;
    text-align: center;
}

.on {
    background: #fff;
    border-left: 2px solid #4DCC57;
}

::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}