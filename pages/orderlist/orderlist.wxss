@import '../../weui.wxss';
.main {
    width: 100%;
    height: 100vh;
    box-sizing: border-box;
    background-color: #F6F6F6;
}
.noOrderList {
    background: url(https://static-o2o.360buyimg.com/daojia/new/images/icon/search_fail.png) no-repeat center;
    background-size: cover;
    width: 200rpx;
    height: 200rpx;
    display: inline-block;
    margin-left: 50%;
    transform: translateX(-50%);
    margin-top: 50%;
}
.noOrderText {
    display: inline-block;
    font-size: 32rpx;
    font-weight: normal;
    white-space: nowrap;
    color: rgba(0, 0, 0, 0.6);
    display: inline-block;
    margin-left: 50%;
    transform: translateX(-50%);
}